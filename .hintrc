{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off",
        "image-alt": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 126",
    "not firefox <= 126",
    "not ios_saf <= 17.5",
    "not safari <= 17.5"
  ]
}